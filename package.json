{
  "name": "us-soccer-calendars",
  "version": "4.0.0",
  "description": "iCal files for the 2026 MLS season",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp -r public/mls build/",
    "preview": "vite preview",
    "generate-mls": "node scripts/generateMLS.js",
    "generate-wc26": "node scripts/generateWC26.js",
    "generate": "npm run generate-mls && npm run generate-wc26",
    "generate-and-build": "npm run generate-mls && npm run generate-wc26 && npm run build"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "ics": "^3.7.2",
    "node-fetch": "^2.7.0"
  }
}
